{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ResiliencePolicies": {
    "RetryPolicy": {
      "RetryCount": 2
    },
    "CircuitBreakerPolicy": {
      "HandledEventsAllowedBeforeBreaking": 3,
      "DurationOfBreakInSeconds": 10
    }
  }
}
